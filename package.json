    "@gotoandplay/nighthawk": "^1.4.0",
    "autoprefixer": "^9.7.1",
    "css-loader": "^3.2.0",
    "expose-loader": "^0.7.5",
    "external-svg-sprite-loader": "^5.0.1",
    "file-loader": "^4.2.0",
    "html-validator": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-junit": "^9.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "react-jsx-parser": "^1.21.0",
    "resolve-url-loader": "^3.1.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "sass-resources-loader": "^2.0.1",
    "style-loader": "^1.0.0",
    "stylelint": "^11.1.1",
    "stylelint-webpack-plugin": "^1.0.3",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.1",
    "ts-jest": "^24.1.0",
