{
  "title": "React Base Project",
  "name": "react-base-project",
  "version": "0.0.0",
  "description": "A base project for React apps.",
  "main": "index.js",
  "scripts": {
    "predev": "rimraf app",
    "dev": "webpack --env.styleguide --env.app",
    "dev:styleguide": "webpack --env.styleguide",
    "dev:app": "webpack-dev-server --env.app",
    "prebuild": "rimraf app",
    "build": "webpack -p --env.production --env.styleguide --env.app",
    "build:styleguide": "webpack -p --env.production --env.styleguide",
    "build:app": "webpack -p --env.production --env.app",
    "test": "npm run lint && npm run jest",
    "jest": "node ./test/generate-tests.js && jest",
    "lint": "tslint -p ./tsconfig.json -e **/*.js"
  },
  "author": "gotoAndPlay <info@play.ee> (https://play.ee)",
  "license": "ISC",
  "devDependencies": {
